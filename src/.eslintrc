{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest": true
    },
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "operator-linebreak": "off"
    },
    "extends": [
        "eslint:recommended",
        "google"
    ]
}